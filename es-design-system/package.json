{
    "name": "es-design-system",
    "version": "0.4.5",
    "private": true,
    "scripts": {
        "storybook:serve": "env-cmd -f .env.local nuxt storybook --ci --quiet",
        "dev": "env-cmd -f .env.local nuxt",
        "build:dev": "npm run generate:dev && npm run deploy:dev",
        "build:prod": "npm run generate:prod && npm run deploy:prod",
        "generate:dev": "env-cmd -f .env.dev npm run generate",
        "generate:prod": "env-cmd -f .env.prod npm run generate",
        "generate": "nuxt storybook build -o ./dist",
        "bootstrap": "env-cmd -f .env.prod npm --prefix aws run cdk bootstrap",
        "synth:dev": "env-cmd -f .env.dev npm run synth",
        "synth:prod": "env-cmd -f .env.prod npm run synth",
        "synth": "npm --prefix aws run cdk synth",
        "deploy:dev": "env-cmd -f .env.dev npm run deploy",
        "deploy:prod": "env-cmd -f .env.prod npm run deploy",
        "deploy": " npm --prefix aws run cdk deploy",
        "lint:js": "eslint --ext \".js,.vue,.mdx\" --ignore-path .gitignore .",
        "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
        "lint:md": "markdownlint **/*.md",
        "lint:yml": "yamllint **/*.yml",
        "lint": "npm run lint:js && npm run lint:style && npm run lint:md && npm run lint:yml",
        "lintfix": "npm run lint:js -- --fix && npm run lint:style -- --fix && npm run lint:md -- --fix",
        "lint:staged": "npx --no-install lint-staged",
        "prepare": "husky install",
        "test": "jest --updateSnapshot",
        "test:watch": "npm run test -- --watchAll",
        "plop": "plop",
        "svgo": "svgo -f ./assets/svg/",
        "postinstall": "npm --prefix aws install"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --cache",
        "*.{css,scss,sass,html,vue}": "stylelint",
        "*.md": "markdownlint",
        "*.yml": "yamllint"
    },
    "prettier": "eslint-config-energysage/prettier",
    "dependencies": {
        "@energysage/es-bs-base": "*",
        "@energysage/es-bs-extends": "*",
        "@energysage/es-vue-base": "*",
        "@nuxt/content": "^1.15.1",
        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/i18n": "^7.2.2",
        "bootstrap-vue": "^2.22.0",
        "cookie-universal-nuxt": "^2.1.5",
        "core-js": "^3.23.3",
        "jest-environment-jsdom": "^28.1.1",
        "nuxt": "^2.15.8",
        "vue": "^2.6.14"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-react": "^7.17.12",
        "@nuxt/content": "^1.15.1",
        "@nuxt/postcss8": "^1.1.3",
        "@nuxtjs/eslint-config": "^10.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/storybook": "^4.3.1",
        "@nuxtjs/style-resources": "^1.2.1",
        "@nuxtjs/stylelint-module": "^4.1.0",
        "@nuxtjs/svg": "^0.4.0",
        "@storybook/addon-actions": "^6.5.6",
        "@storybook/addon-console": "^1.2.3",
        "@storybook/addon-controls": "^6.5.9",
        "@storybook/addon-docs": "^6.5.6",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/addon-notes": "^5.3.21",
        "@storybook/addon-viewport": "^6.5.6",
        "@storybook/addons": "^6.5.6",
        "@storybook/theming": "^6.5.6",
        "@vue/test-utils": "^1.3.0",
        "@whitespace/storybook-addon-html": "^5.0.0",
        "autoprefixer": "10.4.5",
        "aws-cdk": "^2.29.1",
        "aws-cdk-lib": "^2.29.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^28.1.1",
        "env-cmd": "^10.1.0",
        "eslint": "^8.18.0",
        "eslint-config-energysage": "^2.2.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-nuxt": "^1.0.1",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-mdx": "^1.17.0",
        "eslint-plugin-nuxt": "^3.1.0",
        "eslint-plugin-storybook": "^0.5.13",
        "eslint-plugin-strict-vue": "^1.0.0",
        "eslint-plugin-vue": "^8.7.1",
        "eslint-plugin-vue-a11y": "0.0.31",
        "eslint-plugin-vuejs-accessibility": "^1.2.0",
        "extract-sass-to-js": "^1.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.1",
        "jest-axe": "^6.0.0",
        "jest-extended": "^2.0.0",
        "lint-staged": "^13.0.3",
        "markdownlint-cli": "^0.31.1",
        "node-sass": "^7.0.1",
        "plop": "^3.1.0",
        "postcss": "^8.4.13",
        "postcss-html": "^1.4.1",
        "postcss-preset-env": "7.0.0",
        "postcss-scss": "^4.0.4",
        "prettier": "^2.7.1",
        "prettier-eslint": "^13.0.0",
        "sass": "^1.53.0",
        "sass-extract": "^2.1.0",
        "sass-extract-loader": "^1.1.0",
        "sass-loader": "^10.2.1",
        "sass-resources-loader": "^2.2.5",
        "storybook-dark-mode": "^1.1.0",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^26.0.0",
        "vue-jest": "^3.0.7",
        "vue-server-renderer": "^2.6.14",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^4.46.0",
        "yaml-lint": "^1.6.0"
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    }
}
