{
    "name": "es-design-system",
    "version": "0.4.36",
    "private": true,
    "scripts": {
        "dev": "env-cmd -f .env.local nuxt",
        "build": "nuxt generate",
        "build:dev": "npm run generate:dev && npm run deploy:dev",
        "build:prod": "npm run generate:prod && npm run deploy:prod",
        "generate:dev": "env-cmd -f .env.dev npm run generate",
        "generate:prod": "env-cmd -f .env.prod npm run generate",
        "generate": "nuxt storybook build -o ./dist",
        "bootstrap": "env-cmd -f .env.prod npm --prefix aws run cdk bootstrap",
        "synth:dev": "env-cmd -f .env.dev npm run synth",
        "synth:prod": "env-cmd -f .env.prod npm run synth",
        "synth": "npm --prefix aws run cdk synth",
        "deploy:dev": "env-cmd -f .env.dev npm run deploy",
        "deploy:prod": "env-cmd -f .env.prod npm run deploy",
        "deploy": " npm --prefix aws run cdk deploy",
        "lint": "npm run lint:js && npm run lint:style",
        "lint:js": "eslint --ext \".js,.vue,.mdx\" --ignore-path ../.gitignore .",
        "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path ../.gitignore",
        "lintfix": "npm run lint:js -- --fix && npm run lint:style -- --fix"
    },
    "prettier": "eslint-config-energysage/prettier",
    "dependencies": {
        "@energysage/es-bs-base": "^0.4.33",
        "@energysage/es-bs-extends": "^0.4.33",
        "@energysage/es-vue-base": "^0.4.36",
        "@nuxt/content": "^1.15.1",
        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/i18n": "^7.2.2",
        "bootstrap-vue": "^2.22.0",
        "cookie-universal-nuxt": "^2.1.5",
        "core-js": "^3.23.3",
        "html-truncate": "^1.2.2",
        "nuxt": "^2.15.8",
        "vue": "^2.7.8",
        "vue-slider-component": "^3.2.18"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/preset-env": "^7.17.10",
        "@nuxt/content": "^1.15.1",
        "@nuxt/postcss8": "^1.1.3",
        "@nuxtjs/eslint-config": "^10.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/style-resources": "^1.2.1",
        "@nuxtjs/stylelint-module": "^4.1.0",
        "@nuxtjs/svg": "^0.4.0",
        "autoprefixer": "10.4.5",
        "aws-cdk": "^2.29.1",
        "aws-cdk-lib": "^2.29.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^28.1.1",
        "env-cmd": "^10.1.0",
        "eslint": "^8.18.0",
        "eslint-config-energysage": "^2.2.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-nuxt": "^1.0.1",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-mdx": "^1.17.0",
        "eslint-plugin-nuxt": "^3.1.0",
        "eslint-plugin-storybook": "^0.5.13",
        "eslint-plugin-strict-vue": "^1.0.0",
        "eslint-plugin-vue": "^8.7.1",
        "eslint-plugin-vue-a11y": "0.0.31",
        "eslint-plugin-vuejs-accessibility": "^1.2.0",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.13",
        "postcss-html": "^1.4.1",
        "postcss-preset-env": "7.0.0",
        "postcss-scss": "^4.0.4",
        "prettier": "^2.7.1",
        "prettier-eslint": "^13.0.0",
        "sass": "^1.53.0",
        "sass-loader": "^10.2.1",
        "sass-resources-loader": "^2.2.5",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^26.0.0",
        "webpack": "^4.46.0"
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    }
}
